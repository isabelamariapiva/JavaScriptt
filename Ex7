    <script>
let precoLitro = Number(prompt("Digite o preço do litro da gasolina:"));
let valorPagamento = Number(prompt("Digite quanto deseja colocar em reais:"));
let litros = valorPagamento / precoLitro;
alert("Você conseguiu colocar " + litros.toFixed(2) + " litros no tanque.");

    </script>
    
