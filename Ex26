<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    <script>
let idade = Number(prompt("Digite sua idade:"));
let temDocumento = prompt("Você tem documento? (sim/não)").toLowerCase();
if (idade >= 18 && temDocumento === "sim") {
  alert("Acesso permitido.");
} else {
  alert("Acesso negado.");
}
    </script>
    
</body>
</html>
