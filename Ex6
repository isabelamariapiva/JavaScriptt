    <script>
let valorHora = Number(prompt("Quanto você ganha por hora?"));
let horasTrabalhadas = Number(prompt("Quantas horas trabalhou no mês?"));
let salario = valorHora * horasTrabalhadas;
alert("Salário no mês: R$ " + salario.toFixed(2));

    </script>
    
