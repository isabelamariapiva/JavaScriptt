<body>
    <script>
let comprimento = Number(prompt("Digite o comprimento da caixa (m):"));
let largura = Number(prompt("Digite a largura da caixa (m):"));
let altura = Number(prompt("Digite a altura da caixa (m):"));

let volume = comprimento * largura * altura;
alert("O volume da caixa é: " + volume.toFixed(2) + " m³");
    </script>
    
</body>
