    <script>
let temp = Number(prompt("Digite a temperatura:"));
if (temp < 15) {
  alert("Está frio.");
} else if (temp <= 25) {
  alert("Temperatura agradável.");
} else {
  alert("Está quente.");
}
    </script>
    
