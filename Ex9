    <script>
let salarioInicial = Number(prompt("Digite o sal치rio do funcion치rio:"));
let salarioAumento = salarioInicial * 1.15; // aumento de 15%
let salarioFinal = salarioAumento * 0.92; // desconto de 8%

alert(
  "Sal치rio inicial: R$ " + salarioInicial.toFixed(2) + 
  "\nCom aumento: R$ " + salarioAumento.toFixed(2) + 
  "\nSal치rio final: R$ " + salarioFinal.toFixed(2)
);
