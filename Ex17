<body>
    <script>
let nota = Number(prompt("Digite a nota (0 a 10):"));
if (nota < 0 || nota > 10) {
  alert("Nota inválida.");
} else if (nota < 5) {
  alert("Reprovado");
} else if (nota < 7) {
  alert("Recuperação");
} else {
  alert("Aprovado");
}
    </script>
    
</body>
</html>
